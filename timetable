	<script type='text/javascript'>
		
		var applicationPath = "/Timetable-main";
		var currentControllerName = "Timetable";
		
$(document).ready(function () {
	
	var today = new Date();
	var tmpYear  = today.getFullYear();
	var tmpMonth = today.getMonth();
	var tmpDay   = today.getDate();
	var tmpView  = 'agendaWeek';
	if(isSmallMobile()){
		var tmpView  = 'agendaDay';
	}
	var vars = window.location.hash.substr(1).split("&");

	for (var i = 0; i < vars.length; i++) {
		var keyValue = vars[i].split("=");
		if (keyValue[0] == "year")  tmpYear  = keyValue[1];
		if (keyValue[0] == "month") tmpMonth = keyValue[1] - 1;
		if (keyValue[0] == "day")   tmpDay   = keyValue[1];
		if (keyValue[0] == "view")  tmpView  = keyValue[1];
	}

	$('#calendar').fullCalendar({
		
		year:  tmpYear,
		month: tmpMonth,
		date:  tmpDay,
		defaultView: tmpView,
		slotEventOverlap: false,
		
		eventClick: function(calEvent, jsEvent){ 
			/*if (calEvent.dummyUrl!=null)
			{
				window.open(calEvent.dummyUrl);
			    return false;
			}*/
			eventPopUp(calEvent);
        },

		
		
		viewDisplay: function (view) {
			var d = $('#calendar').fullCalendar('getDate');
			if (d.toDateString() == new Date().toDateString())
				window.location.hash = 'view=' + view.name;
			else
				window.location.hash = 'year=' + d.getFullYear() + '&month=' + (d.getMonth() + 1) +
					'&day=' + d.getDate() + '&view=' + view.name;
		},

				header: {
					left: 'prev,next today',
					center: 'title',
					right: 'month,agendaWeek,agendaDay'
				},
				height: 500,
				allDayDefault: false,
				minTime: 8,
				maxTime: 19,
				weekends: false,
				titleFormat: {
					month: 'MMMM yyyy',
					week: "dS[ MMM][ yyyy]{ '&#8212;' dS MMM yyyy}",
					day: 'dS MMMM yyyy'
				},
				columnFormat: {
					month: 'ddd',
					week: 'ddd dS',
					day: 'dddd'
				},
				timeFormat: 'h(:mm)tt',
				eventRender: function(event, element, view) {
					if (view.name == 'month')
						element.find('.fc-event-time').append(' - ' + $.fullCalendar.formatDate(event.end, 'h(:mm)tt'));
					else
						element.find('.fc-event-inner').attr('style', 'background-color:' + event.mainColor + '; border-color:' + event.color + '; color: black');

					if(event.room == ''){
						event.room = '<div class="event-room" style="color:' + event.color +'">NONE</div>';
						event.roomVal = '';
					}
					element.find('.fc-event-time').after('<div title="' + event.building + '" class="event-location">' + event.room + '</div>');
					if (view.name == 'agendaWeek')
						element.find('.fc-event-title')
							.attr('title', event.moduleDesc);
					else
						element.find('.fc-event-title')
							.append(" " + event.moduleDesc)
							.after('<span class="left">' + event.description + '</span>');
					element.find('.fc-event-title')
						.addClass("left")
						.after('<div class="lecturer">' + (view.name == 'month' ? '' : ('&nbsp;' + event.lecturer)) + '</div>');
					element.find('.event-location').attr('style', 'background-color:' + event.color + '; color: #fff');
					element.find('.fc-event-time').attr('style', 'background-color:' + event.color + '; color: #fff');
				},
				events: [
{

ourEventId: '25614',
color: '#CC7800',				//Border and Head
mainColor: '#FFDFB2',		//Event Details
title: '300COM (LECTURE) , 303COM (LECTURE) ',
