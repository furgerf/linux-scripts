	$('#calendar').ready(function () {
		var helphtml = '<span class =\"fc-button fc-button-help fc-state-default fc-corner-left fc-corner-right \">' +
							'<span class="fc-button-inner">' +
								'<span class="fc-button-content">' +
									'help' +
								'</span>' +
								'<span class="fc-button-effect"><span></span></span>' +
							'</span>' +
						'</span>'+
						'<span class="fc-header-space"></span>';

		$('.fc-header-right').prepend(helphtml);
		$('.fc-button-help').click(handleFullcalendarHelp);
		//Handle Month View overflow on last day
		$('.fc-button' ).click(function() {
			$(window).trigger("resize"); 
		});
		$(window).trigger("resize"); 
	});
	
	function handleFullcalendarHelp() {
		var url = applicationPath + "/" + currentControllerName + "/" + "Help";
		 var wWidth = $(window).width();
        var dWidth = wWidth * 0.8;
        var wHeight = $(window).height();
        var dHeight = wHeight * 0.8;
		
		$.get(url, function (data) {
			$('#popup').dialog({
				modal: true,
				resizable: true,
				title: 'Help',
				width: dWidth,
				height: dHeight,
				open: function () {
					/*$.mini_dialog_overlay();*/
					$('#popup').html(data);
				}
			});
		$('#popup').dialog('option', 'position', 'center');
		});
	}	


	function eventPopUp(calEvent){
		//get width of browser window
		var wWidth = $(window).width();
		var dWidth = wWidth * 0.8;
		//get hour of start date
		var stHr = calEvent.start.getUTCHours();
		//calculate if there is a gmt offset required then deduct or add to start hour
		var gmtTimeZoneOffset = new Date(calEvent.start).getTimezoneOffset().toString();
		if(gmtTimeZoneOffset.charAt(0) == '-'){
			stHr = stHr + (gmtTimeZoneOffset.substring(1) / 60);
		}else{
			stHr = stHr - (gmtTimeZoneOffset / 60);
		}
		//calculate if event start hour is AM or PM
		var stDesc = 'AM';
		if(stHr >= 12) {
			if(stHr != 12){
				stHr -= 12;
			}
			var stDesc = 'PM';
		}
		//get hour of end date
		var endHr = calEvent.end.getUTCHours();
		//calculate if there is a gmt offset required then deduct or add to end hour
		var gmtTimeZoneOffset = new Date(calEvent.end).getTimezoneOffset().toString();
		if(gmtTimeZoneOffset.charAt(0) == '-'){
			endHr = endHr + (gmtTimeZoneOffset.substring(1) / 60);
		}else{
			endHr = endHr - (gmtTimeZoneOffset / 60);
		}
		//calculate if event end hour is AM or PM
		var endDesc = 'AM';
		if(endHr >= 12) {
			if(endHr != 12){
				endHr -= 12;
			}
			var endDesc = 'PM';
		}
		//add 'Attendance Registers' link if user is a registered editor
		var registersLink = "";
		if (calEvent.dummyUrl!=null){
			registersLink = '<p style=\"width:100%;text-align:center;\"><a href=\"#\" style=\"font-weight:bold;\" onclick=\"window.open(\''+calEvent.dummyUrl+'\');return false;\">ATTENDANCE REGISTER</a></p>';
		}
		//if no room val then hardcode as empty space
		if(calEvent.roomVal == ''){
			calEvent.room = '';
		}
		//create pop up
		$('#popup').dialog({
			modal: true,
			resizable: false,
			title: $.datepicker.formatDate('DD, dd/mm/yy',calEvent.start)+" "+stHr+stDesc+" - "+endHr+endDesc,
			width: dWidth,
			open: function () {

				$('.ui-widget-overlay').click(function() { $("#popup").dialog("close"); });
				$('.ui-dialog > .ui-widget-header').css('background',calEvent.color);
				$('.ui-dialog > .ui-widget-header').css('color','#fff');
				$('#popup').html('<div><span style=\"font-weight:bold\">Time:</span> '+stHr+stDesc+' - '+endHr+endDesc+'</div>'+
								'<div><span style=\"font-weight:bold\">Title:</span> '+calEvent.moduleDesc+'</div>'+
								'<div><span style=\"font-weight:bold\">Description:</span> '+calEvent.description+'</div>'+
								'<div><span style=\"font-weight:bold\">Room:</span> '+calEvent.room+'</div>'+
								'<div><span style=\"font-weight:bold\">Lecturer:</span> '+calEvent.lecturer+'</div>'+
								'<div><span style=\"font-weight:bold\">Building:</span> '+calEvent.building+'</div>'+
								'<div style=\"color:#fff;\"><span>Event ID:</span> '+calEvent.ourEventId+'</div>'+
								/*'<div style=\"color:#fff;\"><span>Event Type:</span> '+calEvent.eventType+'</div>'+*/
								registersLink);
			}
		});
	}
	//detect if the browser is small mobile (currently < 480px)
	function isSmallMobile(){
		return $('#smallMobileDetector').css('display') === 'none'; 
	}

});

	</script>
	

</head>
<body>
	<div id='calendar' style='margin:1em 0;font-size:13px;'></div>
	<div class="note-text">
